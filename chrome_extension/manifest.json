{
  "name": "Secure pass project",
  "description": "Guarding your data!",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "notifications",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
